<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>LoginFilter</filter-name>
		<display-name>LoginFilter</display-name>
		<description></description>
		<filter-class>com.hand.filter.LoginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LoginFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<display-name>LoginServlet</display-name>
		<description></description>
		<servlet-class>com.hand.web.LoginServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FilmAdd</servlet-name>
		<display-name>FilmAdd</display-name>
		<description></description>
		<servlet-class>com.hand.web.FilmAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FilmList</servlet-name>
		<display-name>FilmList</display-name>
		<description></description>
		<servlet-class>com.hand.web.FilmAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FilmDel</servlet-name>
		<display-name>FilmDel</display-name>
		<description></description>
		<servlet-class>com.hand.web.FilmAction</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>FilmMod</servlet-name>
		<display-name>FilmMod</display-name>
		<description></description>
		<servlet-class>com.hand.web.FilmAction</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FilmAdd</servlet-name>
		<url-pattern>/FilmAction/add</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FilmList</servlet-name>
		<url-pattern>/FilmAction/list</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FilmDel</servlet-name>
		<url-pattern>/FilmAction/delete</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>FilmMod</servlet-name>
		<url-pattern>/FilmAction/modify</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>





</web-app>
